{
  "name": "@form-atoms/field",
  "packageManager": "yarn@4.10.3",
  "version": "0.0.0-semantic-release",
  "author": "Miroslav Petrik <miroslavpetrik@outlook.com>",
  "homepage": "https://github.com/form-atoms/field#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/form-atoms/field.git"
  },
  "bugs": "https://github.com/form-atoms/field/issues",
  "license": "MIT",
  "keywords": [
    "react atom field",
    "react field",
    "react input",
    "form atoms",
    "form atoms field",
    "form fields",
    "jotai",
    "jotai form",
    "jotai form fields",
    "react",
    "react form",
    "react hook field"
  ],
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --build ./tsconfig.build.json --force",
    "test": "vitest",
    "test:cov": "vitest --coverage",
    "typecheck": "vitest --typecheck.only",
    "storybook": "yarn storybook:styles && storybook dev",
    "storybook:build": "yarn storybook:styles && storybook build --docs",
    "storybook:docs": "yarn storybook:styles && storybook dev --docs",
    "storybook:styles": "cp ./node_modules/@picocss/pico/css/pico.colors.min.css public/",
    "storybook:upgrade": "yarn dlx storybook@latest upgrade",
    "lint": "eslint --ext .tsx,.ts ./src",
    "lint:fix": "yarn run lint --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "semantic-release": "semantic-release",
    "upgrade-packages": "yarn upgrade-interactive",
    "upgrade-yarn": "yarn set version stable && yarn install"
  },
  "devDependencies": {
    "@emotion/react": "^11.14.0",
    "@form-atoms/list-atom": "^2.3.11",
    "@picocss/pico": "^2.1.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.0",
    "@semantic-release/npm": "^13.1.1",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@storybook/addon-docs": "^9.1.13",
    "@storybook/addon-links": "^9.1.13",
    "@storybook/react-vite": "^9.1.13",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/lodash.shuffle": "^4.2.9",
    "@types/react": "^18",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^4.0.0",
    "eslint": "9.38.0",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-storybook": "9.1.13",
    "form-atoms": "^3.3.2",
    "happy-dom": "^20.0.8",
    "jotai": "2.15.0",
    "jotai-devtools": "0.8.0",
    "jotai-effect": "2.1.3",
    "lodash.shuffle": "^4.2.0",
    "prettier": "3.6.2",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "remark-gfm": "^4.0.1",
    "semantic-release": "^25.0.1",
    "shiki": "^3.13.0",
    "storybook": "^9.1.13",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.1.11",
    "vitest": "^4.0.0",
    "zod": "4.1.12"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "next",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version}\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "peerDependencies": {
    "@form-atoms/list-atom": "^2",
    "form-atoms": "^3",
    "jotai": "^2",
    "jotai-effect": "^2",
    "react": ">=16.8",
    "zod": "^3"
  }
}
